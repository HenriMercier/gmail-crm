  {
    "name": "Gmail CRM",
    "version": "0.1",
    "description": "Adds buttons in Gmail to link to CRM",
    "permissions": [
      "activeTab"
    ],
    "content_scripts": [
	    {
		    "matches": ["https://mail.google.com/*"],
		    "run_at": "document_idle",
			"js": ["contentScript.js"]
  	 	}
   	],
    "options_page": "options.html",
    "manifest_version": 2
  }